<!DOCTYPE html>
<html>

<head>
  <%- include('../components/head.ejs') %>
</head>

<body>
  <main>
    <ul>
      <% stampedData.forEach(location=>{%>
      <li class="" data-id="<%= location.id %>">
        <form action="/getSavedStamped" method="GET">
          <input type="hidden" name="id" value="<%=location.id %>"><br>
          <input type="submit" value="<%= location.city_name %>">
        </form>

        <img src="<%= location.special %>">


        <form action="/deleteStamped/<%=location.id%>?_method=DELETE" method="POST">
          <button type="submit">REMOVE THIS LOCATION</button>
        </form>
        <%})%>

    </ul>
  </main>
</body>

</html>